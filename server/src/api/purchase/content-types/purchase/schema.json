{
  "kind": "collectionType",
  "collectionName": "purchases",
  "info": {
    "singularName": "purchase",
    "pluralName": "purchases",
    "displayName": "Purchase",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "coupons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::coupon.coupon",
      "mappedBy": "purchase"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "purchase"
    },
    "cards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::card.card",
      "mappedBy": "purchase"
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::client.client",
      "inversedBy": "purchases"
    },
    "cartOrders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::card-order.card-order",
      "mappedBy": "purchase"
    },
    "totalValue": {
      "type": "decimal"
    },
    "freteValue": {
      "type": "decimal",
      "default": 0
    },
    "fretePrazo": {
      "type": "integer"
    },
    "freteRegiao": {
      "type": "string"
    },
    "purchaseStatus": {
      "type": "enumeration",
      "enum": [
        "EM_PROCESSAMENTO",
        "APROVADA", 
        "REPROVADA",
        "EM_TRANSITO",
        "ENTREGUE",
        "CANCELADA"
      ]
    },
    "purchaseSalesStatus": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::purchase-sales-status.purchase-sales-status",
      "inversedBy": "purchases"
    },
    "trade": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::trade.trade",
      "inversedBy": "purchases"
    },
    "date": {
      "type": "date"
    },
    "cardPaymentData": {
      "type": "text",
      "required": false
    }
  }
}
